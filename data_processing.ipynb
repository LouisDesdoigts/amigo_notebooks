    "import jax\n",
    "from amigo.pipelines import process_calslope\n",
    "\n",
    "jax.config.update(\"jax_enable_x64\", True)\n",
    "\n",
    "path = \"../../data\"\n",
    "programs = [\"GO1843\"]\n",
    "\n",
    "for program in programs:\n",
    "    process_calslope(f\"{path}/uncal/{program}/\", f\"{path}/calslope/{program}/\")"
